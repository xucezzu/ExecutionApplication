<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.bubble.execute">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_application"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon_application"
        android:supportsRtl="true"
        android:name="com.bubble.execute.ExApplication"
        android:theme="@style/AppTheme">
        <activity android:name=".view.activity.SplashPageActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".view.activity.LoginActivity"/>
        <activity android:name=".view.activity.MainActivity"/>
        <activity android:name=".view.activity.PasswordActivity"/>
    </application>

</manifest>